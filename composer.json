			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"\Documents\PHPStorm\php-5.6.22-nts-Win32-VC11-x86\php artisan clear-compiled",
			"\Documents\PHPStorm\php-5.6.22-nts-Win32-VC11-x86\php artisan optimize"
		],
		"post-update-cmd": [
			"\Documents\PHPStorm\php-5.6.22-nts-Win32-VC11-x86\php artisan clear-compiled",
			"\Documents\PHPStorm\php-5.6.22-nts-Win32-VC11-x86\php artisan optimize"
		],
		"post-create-project-cmd": [
			"\Documents\PHPStorm\php-5.6.22-nts-Win32-VC11-x86\php artisan key:generate"
		]
	},
